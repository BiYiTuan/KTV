<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Ktv">
        
        <attr name="shadowImage" format="reference" />
    	<attr name="defaultImage" format="reference" />
    	<attr name="defaultText" format="string" /> 
    	<attr name="type" format="integer" />
    	<attr name="shadowPaddingLeft" format="dimension"/>
    	<attr name="shadowPaddingRight" format="dimension" />
    	<attr name="shadowPaddingTop" format="dimension" />
    	<attr name="shadowPaddingBottom" format="dimension" />
        
        
    	<attr name="backgroundImage" format="reference" />
    	<attr name="order" format="integer" />
    	<attr name="YDelta" format="dimension" />
    	<attr name="menuImage" format="reference" />
    	<attr name="nav_line_focus" format="reference" />
    	<attr name="nav_line_unfocus" format="reference" />
    	<attr name="nav_layout" format="reference" />
    	<attr name="movedirect">
	        <flag name="left" value="0x00000000" />
	        <flag name="right" value="0x00000001" />
	        <flag name="top" value="0x00000002" />
	        <flag name="bottom" value="0x00000002" />
    	</attr>
    	<attr name="movestep" format="float" />
    	<attr name="rotatedirect">
	        <flag name="cw" value="0x00000000" />
	        <flag name="ccw" value="0x00000001" />
    	</attr>
    	
    	<attr name="deltaDegree" format="integer" />
    	<attr name="contentImage" format="reference" />
    	<attr name="tagImage" format="reference" />
    	<attr name="title" format="reference" />
    
	    <attr name="sidebuffer" format="integer" />
	    <attr name="fillColor" format="color" />
	    <attr name="strokeColor" format="color" />
	    <attr name="radius" format="integer" />
	    <attr name="centered" format="boolean" />
	    <attr name="developerTextSize" format="float" />
	    <attr name="nameTextSize" format="float" />
	    <attr name="scoreHeight" format="integer" />
	    <attr name="titlePadding" format="integer" />
	    <attr name="selectedColor" format="color" />
	    <attr name="selectedBold" format="boolean" />
	    <attr name="textColor" format="color" />
	    <attr name="textSize" format="float" />
	    <attr name="footerLineHeight" format="integer" />
	    <attr name="footerColor" format="color" />
	    <attr name="footerTriangleHeight" format="integer" />
	    <attr name="footerRectHeight" format="integer" />
	    <attr name="footerRectWidth" format="integer" />
    
    </declare-styleable>
    
</resources>
