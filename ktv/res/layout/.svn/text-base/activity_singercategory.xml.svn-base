<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clipChildren="false" >

    <TextView
        android:id="@+id/id_singercategory_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="95dp"
        android:layout_marginTop="40dp"
        android:text="@string/categorysinger_title"
        android:textColor="@color/umlist_normal_text"
        android:textSize="38sp" />

    <TextView
        android:id="@+id/id_singercategory_keystr"
        android:layout_width="406dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="834dp"
        android:layout_marginTop="55dp"
        android:gravity="center_vertical"
        android:hint="@string/searchsinger_keyword_hint"
        android:maxLength="10"
        android:textColor="@color/umlist_check_text"
        android:textColorHint="@color/umlist_hint_text"
        android:textSize="36sp"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/id_singercategory_icon"
        android:layout_width="70dp"
        android:layout_height="50dp"
        android:layout_marginStart="1130dp"
        android:layout_marginTop="56dp"
        android:contentDescription="@string/app_name"
        android:focusable="false"
        android:scaleType="fitXY"
        android:src="@drawable/singer_search_unselected"
        android:visibility="gone" />

    <View
        android:layout_width="324dp"
        android:layout_height="2dp"
        android:layout_marginStart="827dp"
        android:layout_marginTop="101dp"
        android:background="@drawable/line_singer_search"
        android:visibility="gone" />

    <com.sz.ead.app.ktv.ui.widget.ImageRotation
        android:id="@+id/id_singercategory_imagerotation"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_centerInParent="true"
        android:src="@drawable/search_loading" />

    <ImageView
        android:id="@+id/id_singercategory_focus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="94dp"
        android:contentDescription="@string/app_name"
        android:scaleType="fitXY"
        android:src="@drawable/category_focus"
        android:visibility="invisible" />

    <RelativeLayout
        android:id="@+id/id_singercategory_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <com.sz.ead.app.ktv.ui.widget.SingerCategoryView
            android:id="@+id/id_singercategory_item0"
            android:layout_width="200dp"
            android:layout_height="240dp"
            android:layout_marginLeft="60dp"
            android:layout_marginTop="124dp"
            android:focusable="true" />

        <com.sz.ead.app.ktv.ui.widget.SingerCategoryView
            android:id="@+id/id_singercategory_item1"
            android:layout_width="200dp"
            android:layout_height="240dp"
            android:layout_alignTop="@+id/id_singercategory_item0"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/id_singercategory_item0"
            android:focusable="true" />

        <com.sz.ead.app.ktv.ui.widget.SingerCategoryView
            android:id="@+id/id_singercategory_item2"
            android:layout_width="200dp"
            android:layout_height="240dp"
            android:layout_alignTop="@+id/id_singercategory_item1"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/id_singercategory_item1"
            android:focusable="true" />

        <com.sz.ead.app.ktv.ui.widget.SingerCategoryView
            android:id="@+id/id_singercategory_item3"
            android:layout_width="200dp"
            android:layout_height="240dp"
            android:layout_alignTop="@+id/id_singercategory_item2"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/id_singercategory_item2"
            android:focusable="true" />

        <com.sz.ead.app.ktv.ui.widget.SingerCategoryView
            android:id="@+id/id_singercategory_item4"
            android:layout_width="200dp"
            android:layout_height="240dp"
            android:layout_alignTop="@+id/id_singercategory_item3"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/id_singercategory_item3"
            android:focusable="true" />

        <com.sz.ead.app.ktv.ui.widget.SingerCategoryView
            android:id="@+id/id_singercategory_item5"
            android:layout_width="200dp"
            android:layout_height="240dp"
            android:layout_alignLeft="@+id/id_singercategory_item0"
            android:layout_below="@+id/id_singercategory_item0"
            android:layout_marginTop="40dp"
            android:focusable="true" />

        <com.sz.ead.app.ktv.ui.widget.SingerCategoryView
            android:id="@+id/id_singercategory_item6"
            android:layout_width="200dp"
            android:layout_height="240dp"
            android:layout_alignTop="@+id/id_singercategory_item5"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/id_singercategory_item5"
            android:focusable="true" />

        <com.sz.ead.app.ktv.ui.widget.SingerCategoryView
            android:id="@+id/id_singercategory_item7"
            android:layout_width="200dp"
            android:layout_height="240dp"
            android:layout_alignTop="@+id/id_singercategory_item6"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/id_singercategory_item6"
            android:focusable="true" />

        <com.sz.ead.app.ktv.ui.widget.SingerCategoryView
            android:id="@+id/id_singercategory_item8"
            android:layout_width="200dp"
            android:layout_height="240dp"
            android:layout_alignTop="@+id/id_singercategory_item7"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/id_singercategory_item7"
            android:focusable="true" />

        <com.sz.ead.app.ktv.ui.widget.SingerCategoryView
            android:id="@+id/id_singercategory_item9"
            android:layout_width="200dp"
            android:layout_height="240dp"
            android:layout_alignTop="@+id/id_singercategory_item8"
            android:layout_marginLeft="40dp"
            android:layout_toRightOf="@+id/id_singercategory_item8"
            android:focusable="true" />
    </RelativeLayout>

    <com.sz.ead.app.ktv.ui.widget.Indicator
        android:id="@+id/id_singercategory_indicator"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:focusable="true"
        android:visibility="visible" />

    <com.sz.ead.app.ktv.ui.widget.EmptyView
        android:id="@+id/id_singercategory_empty"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:contentDescription="@string/app_name"
        android:visibility="gone" />

</RelativeLayout>